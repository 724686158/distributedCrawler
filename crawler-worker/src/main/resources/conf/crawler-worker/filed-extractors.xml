<?xml version="1.0" encoding="UTF-8"?>
<ext:components-config version="1.0"
	xmlns:ext="http://guang.org/distributedCrawler/components" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://guang.org/distributedCrawler/components http://guang.org/distributedCrawler/url-extractors ">
	<ext:component-defines>
		<ext:component>
			<ext:component-name>default</ext:component-name>
			<ext:component-class>guang.crawler.extension.filedExtractor.DefaultFieldExtractor</ext:component-class>
			<ext:load-on-startup>true</ext:load-on-startup>
		</ext:component>
		<ext:component>
			<ext:component-name>QQCommentFieldsExtractor</ext:component-name>
			<ext:component-class>guang.crawler.extension.filedExtractor.qq.QQCommentFieldsExtractor</ext:component-class>
			<ext:load-on-startup>false</ext:load-on-startup>
		</ext:component>
		<ext:component>
			<ext:component-name>QQCommentCountFieldsExtractor</ext:component-name>
			<ext:component-class>guang.crawler.extension.filedExtractor.qq.QQCommentCountFieldsExtractor</ext:component-class>
		</ext:component>
	</ext:component-defines>
	<ext:component-mappings>
		<ext:component-mapping>
			<ext:component-name>QQCommentFieldsExtractor</ext:component-name>
			<ext:url-pattern>http://coral\.qq\.com/article/[0-9]+/comment\?commentid=[0-9]+.*</ext:url-pattern>
		</ext:component-mapping>
		<ext:component-mapping>
			<ext:component-name>QQCommentCountFieldsExtractor</ext:component-name>
			<ext:url-pattern>http://coral\.qq\.com/article/[0-9]+/commentnum\??.*</ext:url-pattern>
		</ext:component-mapping>
	</ext:component-mappings>
</ext:components-config>
