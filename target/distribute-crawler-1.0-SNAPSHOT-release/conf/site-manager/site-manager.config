crawler.site-manager.workdir=/home/yang
# 一个作业的超时时间设置为5分钟
crawler.site-manager.job.timeout=300000
# 一个作业的尝试执行次数
crawler.site-manager.job.tryTime=3
# 清理器清理的时间间隔，设置为100s
crawler.site-manager.queue.cleaner.period=100000
# 备份的时间间隔，设置为1个小时
crawler.site-manager.backuper.backup.period=3600000
# 进行备份时使用的用户
crawler.site-manager.hadoop.user=zhou
# 进行备份的HDFS的连接
crawler.site-manager.hadoop.url=hdfs://ubuntu-3:8020
# zookeeper 连接
crawler.site-manager.zookeeper.url=ubuntu-3,ubuntu-6,ubuntu-8
# 进行备份的HDFS路径
crawler.site-manager.hadoop.path=/home/crawler

