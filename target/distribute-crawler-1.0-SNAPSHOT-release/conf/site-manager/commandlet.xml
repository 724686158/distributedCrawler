<?xml version="1.0" encoding="UTF-8"?>
<cmdlet:components-config
	xmlns:cmdlet="http://guang.org/distributedCrawler/components"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://guang.org/distributedCrawler/components http://guang.org/distributedCrawler/url-extractors ">
	<cmdlet:component-defines>
		<cmdlet:component>
			<cmdlet:component-name>URLsGetter</cmdlet:component-name>
			<cmdlet:component-class>guang.crawler.siteManager.commandlet.URLsGetter</cmdlet:component-class>
			<cmdlet:load-on-startup>false</cmdlet:load-on-startup>
		</cmdlet:component>
		<cmdlet:component>
			<cmdlet:component-name>URLsPutter</cmdlet:component-name>
			<cmdlet:component-class>guang.crawler.siteManager.commandlet.URLsPutter</cmdlet:component-class>
			<cmdlet:load-on-startup>false</cmdlet:load-on-startup>
		</cmdlet:component>
		<cmdlet:component>
			<cmdlet:component-name>StatisticsGetter</cmdlet:component-name>
			<cmdlet:component-class>guang.crawler.siteManager.commandlet.StatisticsGetter</cmdlet:component-class>
			<cmdlet:load-on-startup>false</cmdlet:load-on-startup>
		</cmdlet:component>
	</cmdlet:component-defines>
	<cmdlet:component-mappings>
		<cmdlet:component-mapping>
			<cmdlet:component-name>URLsGetter</cmdlet:component-name>
			<cmdlet:url-pattern>/url/get</cmdlet:url-pattern>
		</cmdlet:component-mapping>
		<cmdlet:component-mapping>
			<cmdlet:component-name>URLsPutter</cmdlet:component-name>
			<cmdlet:url-pattern>/url/put</cmdlet:url-pattern>
		</cmdlet:component-mapping>
	</cmdlet:component-mappings>
</cmdlet:components-config>
